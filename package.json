{
  "author": "Juxhin Bleta",
  "name": "x-react-web-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:style": "postcss src/theme/style/style-raw.scss -o src/theme/style/style.scss",
    "start-scss-map": "react-app-rewired start node-sass --source-map true --include-path ./src --include-path ./node_modules ./src/scss -o",
    "start": "npm run build:style && react-app-rewired start",
    "build": "npm run build:style && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "storybook": "start-storybook",
    "format-all": "npx prettier --write 'src/**/*'"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "emotion": "^11.0.0",
    "formik": "^2.2.9",
    "node-sass": "^4.12.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-localization": "^1.0.15",
    "react-scripts": "^5.0.1",
    "sass-loader": "^8.0.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^1.1.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@storybook/react": "^6.5.13",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.25",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "@welldone-software/why-did-you-render": "^4.0.3",
    "autoprefixer": "^9.7.4",
    "babel-jest": "^24.9.0",
    "customize-cra": "^0.9.1",
    "postcss-cli": "^9.0.2",
    "prettier": "^2.5.1",
    "react-app-rewired": "^2.1.8",
    "react-router-dom": "^6.0.2",
    "typescript": "^4.5.3"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "@typescript-eslint/consistent-type-assertions": "off",
      "@typescript-eslint/no-unused-expressions": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
